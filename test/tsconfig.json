{
    "exclude": [
        "node_modules"
    ],
    "compilerOptions": {
        "target": "esnext",
        "module": "commonjs",
        "moduleResolution": "node",
        "allowJs": false,
        "checkJs": false,
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "baseUrl": "./",
        "paths": {
            "alpaca-addon": ["node_modules/alpaca-addon"]
        }
    }
}